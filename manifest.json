{
  "manifest_version": 3,
  "name": "YT-downloader",
  "description": "Extention fetches video url and downloads it",
  "version": "1.0",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "assets/yt_logo.png"
  },
  "permissions": ["tabs", "activeTab"]
}
